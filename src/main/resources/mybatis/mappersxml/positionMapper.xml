<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.solvd.dao.interfaces.IPositionsDAO">
    <resultMap id="positions" type="com.solvd.model.PositionModel">
        <id property="id" column="id"/>
        <result property="name" column="name"/>
    </resultMap>
    <select id="getAllPosition" resultMap="positions">
        SELECT * FROM positions;
    </select>
</mapper>