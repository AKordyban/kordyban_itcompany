<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.solvd.dao.interfaces.ILocationsDAO">
    <resultMap id="location" type="com.solvd.model.LocationModel">
        <id property="id" column="id"/>
        <result property="name" column="name"/>
    </resultMap>
    <select id="getAllLocation" resultMap="location">
        SELECT * FROM location;
    </select>
</mapper>