<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.solvd.dao.interfaces.IOrdersDAO">
    <resultMap id="orders" type="com.solvd.model.OrdersModel">
        <id property="id" column="id"/>
        <result property="orderDate" column="orderDate"/>
        <result property="quantity" column="quantity"/>
        <result property="price" column="price"/>
        <result property="equipment_id" column="equipment_id"/>
        <result property="clients_id" column="clients_id"/>
        <result property="location_id" column="location_id"/>
        <result property="employees_id" column="employees_id"/>
        <result property="products_id" column="products_id"/>
    </resultMap>
    <select id="getAllOrder" resultMap="orders">
        SELECT * FROM orders;
    </select>
</mapper>