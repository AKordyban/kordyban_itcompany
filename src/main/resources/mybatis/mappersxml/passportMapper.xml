<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.solvd.dao.interfaces.IPassportsDAO">
    <resultMap id="passport" type="com.solvd.model.PassportModel">
        <id property="id" column="id"/>
        <result property="seria" column="seria"/>
        <result property="code" column="code"/>
    </resultMap>
    <select id="getAllPassport" resultMap="passport">
        SELECT * FROM passport;
    </select>
</mapper>